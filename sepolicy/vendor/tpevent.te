type tpevent, domain;
type tpevent_exec, file_type, exec_type, vendor_file_type;

init_daemon_domain(tpevent)

allow tpevent { input_device touchfeature_device}:chr_file rw_file_perms;
allow tpevent { proc_interrupts proc_tp_lockdown }:file r_file_perms;
allow tpevent input_device:dir r_dir_perms;
allow tpevent sysfs_touch_hostprocess:file rw_file_perms;
allow tpevent touchfeature_device:file { read getattr map };
